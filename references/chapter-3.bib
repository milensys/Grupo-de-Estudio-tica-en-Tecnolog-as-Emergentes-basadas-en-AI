@article{ WOS:000788455300001,
Author = {Wang, Lei and Gopal, Ram and Shankar, Ramesh and Pancras, Joseph},
Title = {Forecasting venue popularity on location-based services using
   interpretable machine learning},
Journal = {PRODUCTION AND OPERATIONS MANAGEMENT},
Year = {2022},
Volume = {31},
