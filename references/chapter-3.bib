Author = {Hao, Nayi and Li, Xinzhou and Han, Danping and Nie, Wenbin},
Title = {Quantifying the Impact of Street Greening during Full-Leaf Seasons on
   Emotional Perception: Guidelines for Resident Well-Being},
Journal = {FORESTS},
Year = {2024},
