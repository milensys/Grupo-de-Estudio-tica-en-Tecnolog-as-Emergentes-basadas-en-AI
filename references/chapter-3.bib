@article{ WOS:001004224400016,
Author = {Masood, Usama and Farooq, Hasan and Imran, Ali and Abu-Dayya, Adnan},
Title = {Interpretable AI-Based Large-Scale 3D Pathloss Prediction Model for
   Enabling Emerging Self-Driving Networks},
Journal = {IEEE TRANSACTIONS ON MOBILE COMPUTING},
Year = {2023},
Volume = {22},
