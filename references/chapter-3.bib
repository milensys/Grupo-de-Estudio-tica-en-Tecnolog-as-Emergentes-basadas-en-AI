@article{ WOS:000939193500001,
Author = {Liu, Fang and Wang, Xiaodi and Li, Ting and Huang, Mingzeng and Hu, Tao
   and Wen, Yunfeng and Su, Yunche},
Title = {An Automated and Interpretable Machine Learning Scheme for Power System
   Transient Stability Assessment},
