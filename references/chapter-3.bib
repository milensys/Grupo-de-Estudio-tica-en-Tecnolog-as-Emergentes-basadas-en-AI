@article{ WOS:001195409200001,
Author = {Gomez-Talal, Ismael and Bote-Curiel, Luis and Rojo-alvarez, Jose Luis},
Title = {Understanding the disparities in Mathematics performance: An
   interpretability-based examination},
Journal = {ENGINEERING APPLICATIONS OF ARTIFICIAL INTELLIGENCE},
